    public async Task LoadProducts(int ProductID, string ProductName, decimal UnitPrice, string CategoryName)
    {

        try
        {



            using (SqlConnection conn = new SqlConnection(this.Variables.ConnectionString))
            {

                using (SqlCommand command = new SqlCommand("dbo.SP_LoadProducts", conn))
                {

                    SqlParameter parameter = new SqlParameter("@ProductID", ProductID);

                    command.Parameters.AddWithValue("@ProductID", ProductID);
                    command.Parameters.AddWithValue("@ProductName", ProductName);
                    command.Parameters.AddWithValue("@UnitPrice", UnitPrice);
                    command.Parameters.AddWithValue("@CategoryName", CategoryName);
                    command.CommandType = CommandType.StoredProcedure;
                    await conn.OpenAsync();
                    await command.ExecuteNonQueryAsync();


                }


            }

        }
        catch (Exception ex)
        {

            Console.WriteLine($"Error de carga de los Customers: {ex.Message}");



        }


    }

